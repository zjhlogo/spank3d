inline Vector2::Vector2(float fx /*= 0.0f*/, float fy /*= 0.0f*/)
{
	x = fx;
	y = fy;
}

inline void Vector2::Reset(float fx /*= 0.0f*/, float fy /*= 0.0f*/)
{
	x = fx;
	y = fy;
}
